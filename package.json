{
  "name": "iris-ai",
  "version": "2.4.0",
  "description": "Iris - Integrated Runtime Intelligence Service with smart provider selection, supporting Ollama, Gemini, and Claude",
  "main": "src/enhanced-ai.js",
  "type": "module",
  "bin": {
    "iris": "./src/enhanced-ai.js"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node src/enhanced-ai.js",
    "test": "node tests/test-runner.js",
    "test:examples": "node examples/basic-usage.js",
    "dev": "node --watch src/enhanced-ai.js",
    "health": "node src/enhanced-ai.js health",
    "models": "node src/enhanced-ai.js models",
    "providers": "node src/enhanced-ai.js providers",
    "postinstall": "node setup.js",
    "setup": "node setup.js",
    "lint": "eslint src/ tests/ examples/",
    "lint:fix": "eslint src/ tests/ examples/ --fix",
    "validate": "npm run lint && npm test",
    "prepack": "npm run validate"
  },
  "keywords": [
    "ai",
    "llm",
    "ollama",
    "gemini",
    "claude",
    "anthropic",
    "cli",
    "multi-provider",
    "automation",
    "machine-learning",
    "nlp",
    "chat"
  ],
  "author": "Jordan After Midnight",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jordanaftermidnight/multi-ai-integration-CLI.git"
  },
  "bugs": {
    "url": "https://github.com/jordanaftermidnight/multi-ai-integration-CLI/issues"
  },
  "homepage": "https://github.com/jordanaftermidnight/multi-ai-integration-CLI#readme",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.3",
    "@google/generative-ai": "^0.21.0",
    "groq-sdk": "^0.26.0",
    "ollama": "^0.5.16",
    "openai": "^5.8.2"
  },
  "devDependencies": {
    "eslint": "^8.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/jordanaftermidnight"
  }
}
